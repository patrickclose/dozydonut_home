{
    "version": 2,
    "builds": [
      {
        "src": "server.cjs",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "style.css",
        "use": "@vercel/static"
      },
      {
        "src": "scripts.js",
        "use": "@vercel/static"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.cjs"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  